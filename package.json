{
  "name": "capstone",
  "version": "3.0.0",
  "description": "How to use create-react-app with a custom Node API on Heroku",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "start": "node server",
    "build": "cd react-ui/ && npm run build",
    "start-dev": "NODE_ENV='development' npm run build & NODE_ENV='development' npm run start-server",
    "start-server": "nodemon server"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "express": "^4.17.1",
    "pg": "^8.3.2",

    "sequelize": "^6.3.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mars/heroku-cra-node.git"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "eslint": "^5.12.0",
    "nodemon": "^2.0.4"
  }
}
